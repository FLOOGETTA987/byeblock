{
  "manifest_version": 3,
  "name": "ByeBlock: SoundCloud Geoblocking Remover",
  "short_name": "ByeBlock",
  "author": "Jari Zwarts",
  "homepage_url": "https://jari.io",
  "description": "ByeBlock attempts to stream tracks through other countries when they are blocked in your own country.",
  "version": "1.0.4",
  "permissions": [
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "byeblock.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "512": "512.png",
    "128": "128.png"
  },
  "content_security_policy": {},
  "host_permissions": [
    "*://a-v2.sndcdn.com/*"
  ]
}